{
    "name": "recaptcha/recaptcha",
    "description": "CakePHP 3 recaptcha Plugin",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "cakephp3", "recaptcha"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Burgess (Adapted by: Leonardo Tatarin)",
            "email": "leonardo.tatarin@connaxis.com"
        }
    ],
    "require": {
		"php": ">=5.4"
    },
    "require-dev": {
		"phpunit/phpunit": "*"
    },
    "autoload": {
	"psr-4": {
		"Recaptcha\\": "src"
	}
    },
    "autoload-dev": {
		"psr-4": {
			"Recaptcha\\Test\\": "tests",
			"Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
			}
    },
    "extra": {
		"installer-name": "Recaptcha"
    }
}